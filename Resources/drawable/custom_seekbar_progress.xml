<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:id="@android:id/background">
		<shape
            android:shape="line">
			<stroke
                android:color="@color/seekbar_track_background"
                android:width="4dp"/>
		</shape>
	</item>
	<item android:id="@android:id/progress">
		<clip>
			<shape
                android:shape="line">
				<stroke
                    android:color="@color/menubar"
                    android:width="4dp"/>
			</shape>
		</clip>
	</item> 
</layer-list>
